ParserManager -> ParseDeleteNoteCommand: <<create>>

activate ParseDeleteNoteCommand
ParserManager <-- ParseDeleteNoteCommand
deactivate ParseDeleteNoteCommand

ParserManager -> ParseDeleteNoteCommand: parse()

activate ParseDeleteNoteCommand
create Command
